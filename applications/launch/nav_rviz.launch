<launch>
  <!-- Define the map_file argument with the default value pointing to your new map -->
  <arg name="map_file" default="/home/louisa/maps/playground.yaml" />

  <!-- Include the fetch_nav.launch file with the map_file argument passed to it -->
  <include file="$(find fetch_navigation)/launch/fetch_nav.launch">
    <arg name="map_file" value="$(arg map_file)" />
  </include>

  <!-- Launch RViz with the appropriate configuration file -->
  <node pkg="rviz" type="rviz" name="$(anon rviz)" args="-d $(find applications)/config/nav.rviz" />
</launch>